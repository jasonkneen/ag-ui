{
  "name": "@ag-ui/mcp-apps-middleware",
  "author": "Markus Ecker",
  "version": "0.0.3",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist coverage",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "link:global": "pnpm link --global",
    "unlink:global": "pnpm unlink --global"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "rxjs": "7.8.1"
  },
  "peerDependencies": {
    "@ag-ui/client": ">=0.0.40"
  },
  "devDependencies": {
    "@ag-ui/client": "workspace:*",
    "@types/node": "^20.11.19",
    "@vitest/coverage-istanbul": "^4.0.18",
    "rxjs": "7.8.1",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  }
}