{
  "name": "@ag-ui/langchain",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist .turbo node_modules coverage",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "link:global": "pnpm link --global",
    "unlink:global": "pnpm unlink --global"
  },
  "dependencies": {
    "@langchain/core": "^0.3.80",
    "rxjs": "7.8.1"
  },
  "peerDependencies": {
    "@ag-ui/core": ">=0.0.42",
    "@ag-ui/client": ">=0.0.42",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@ag-ui/core": "workspace:*",
    "@ag-ui/client": "workspace:*",
    "@types/node": "^20.11.19",
    "@vitest/coverage-istanbul": "^4.0.18",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  }
}